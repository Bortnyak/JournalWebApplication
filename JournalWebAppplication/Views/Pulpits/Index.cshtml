@model IEnumerable<JournalWebAppplication.Models.Pulpit>

@{
    ViewBag.Title = "Index";
}
<p>
    <h2>@ViewBag.FacultyName факультет</h2>
</p>

<table class="table">
    <tr>
        <th>
            Назва кафедри
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PulpitName)
        </td>
        <td>
           @Html.ActionLink("Перейти", "Index", controllerName: "Groups", routeValues: new { id = item.Id }, htmlAttributes: null)
        </td>
    </tr>
}

</table>
